      "package": "com.marzzeh.Drapo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "20884d96-2482-4269-9a75-2ddf3b19df9a"
      }
    }
  }
}
